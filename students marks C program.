*/

#include <stdio.h>

int main(){
  float attendance ,average marks;

  //input attendance percentage
  printf("75%:");
  scanf("%f",& attendance);

  //input average marks
  printf("40:");
  scanf("%f",&average marks);

  //check eligibility
  if (attendance >=75 && average marks >=40){
    printf("Eligible for final exams\n");
} else {
    printf ("Not eligible\n");
}

  return 0;
}
